# Lakebase PostgreSQL (auto-injected by Databricks Apps)
PGHOST=
PGUSER=
PGPASSWORD=
PGDATABASE=databricks_postgres
PGPORT=5432
PGSSLMODE=require

# Databricks (auto-injected by Databricks Apps)
DATABRICKS_HOST=
DATABRICKS_TOKEN=
DATABRICKS_WAREHOUSE_ID=
# Databricks Model Serving endpoints for NL CEP generation (use Sonnet/Opus)
DATABRICKS_LLM_SONNET_ENDPOINT=
DATABRICKS_LLM_OPUS_ENDPOINT=
# Optional generic fallback endpoint name
DATABRICKS_LLM_ENDPOINT=

# Unity Catalog volume for pipeline storage (when DATABRICKS_HOST is set)
DATABRICKS_CATALOG=main
DATABRICKS_SCHEMA=main

# Schema Registry (optional, for Kafka Avro/Protobuf)
SCHEMA_REGISTRY_URL=

# Local development
ENVIRONMENT=development
NEXT_PUBLIC_API_URL=http://localhost:8000
